@article{nagle_exploiting_2013,
	title = {``{E}xploiting Intrinsic Triangular Geometry in Relativistic He3+{Au} Collisions to Disentangle Medium Properties''},
	url = {http://arxiv.org/abs/1312.4565},
	abstract = {Recent results in d+{Au} and p+Pb collisions at {RHIC} and the {LHC} provide evidence for collective expansion and flow of the created medium. We propose a control set of experiments to directly compare particle emission patterns from p+{Au}, d+{Au}, and He3+{Au} or t+{Au} collisions at the same sqrt({sNN}). Using Monte Carlo Glauber we find that a He3 or triton projectile, with a realistic wavefunction description, induces a significant intrinsic triangular shape to the initial medium and that, even with viscous damping, this survives into a significant third order flow moment v3. By comparing systems with one, two, and three initial hot spots, one can disentangle the effects from the initial spatial distribution of the deposited energy and viscous damping. These are key tools to answering the question of how small a droplet of matter is necessary to form a quark-gluon plasma described by nearly inviscid hydrodynamics.},
	journal = {Phys. Rev. Lett.},
	author = {Nagle, J. L. and Adare, A. and Beckman, S. and Koblesky, T. and Koop, J. O. and McGlinchey, D. and Romatschke, P. and Carlson, J. and Lynn, J. E. and McCumber, M.},
        volume = {113},
        issue = {11},
        pages = {112301},
        numpages = {5},
        year = {2014},
        month = {Sep},
	keywords = {Nuclear Experiment, Nuclear Theory},
	file = {arXiv\:1312.4565 PDF:/Users/jdok/Documents/Zotero_Storage/zotero/storage/7G3UQ3A7/Nagle et al. - 2013 - Exploiting Intrinsic Triangular Geometry in Relati.pdf:application/pdf;arXiv.org Snapshot:/Users/jdok/Documents/Zotero_Storage/zotero/storage/6TQGKJSA/1312.html:text/html}
}

